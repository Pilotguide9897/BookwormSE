sages":"62","suppressedMessages":"63","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/index.js",[],[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/App.js",["64"],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/pages/SavedBooks.js",["65","66","67","68"],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/components/Navbar.js",[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/pages/SearchBooks.js",["69","70"],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/components/SignupForm.js",["71"],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/components/LoginForm.js",[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/mutations.js",[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/API.js",[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/localStorage.js",[],[],"/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/auth.js",["72"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/queries.js",[],[],{"ruleId":"73","severity":1,"message":"74","line":8,"column":8,"nodeType":"75","messageId":"76","endLine":8,"endColumn":12},{"ruleId":"73","severity":1,"message":"77","line":1,"column":17,"nodeType":"75","messageId":"76","endLine":1,"endColumn":25},{"ruleId":"73","severity":1,"message":"78","line":1,"column":27,"nodeType":"75","messageId":"76","endLine":1,"endColumn":36},{"ruleId":"73","severity":1,"message":"79","line":12,"column":28,"nodeType":"75","messageId":"76","endLine":12,"endColumn":38},{"ruleId":"73","severity":1,"message":"80","line":12,"column":71,"nodeType":"75","messageId":"76","endLine":12,"endColumn":82},{"ruleId":"73","severity":1,"message":"81","line":32,"column":22,"nodeType":"75","messageId":"76","endLine":32,"endColumn":27},{"ruleId":"73","severity":1,"message":"82","line":95,"column":15,"nodeType":"75","messageId":"76","endLine":95,"endColumn":19},{"ruleId":"73","severity":1,"message":"81","line":15,"column":21,"nodeType":"75","messageId":"76","endLine":15,"endColumn":26},{"ruleId":"83","severity":1,"message":"84","line":49,"column":1,"nodeType":"85","endLine":49,"endColumn":34},"no-unused-vars","'Auth' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","'useEffect' is defined but never used.","'removeData' is assigned a value but never used.","'removeEr