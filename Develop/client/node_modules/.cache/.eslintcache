{\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/jared_green97/bootcamp/Challenges/21-BookwormSE/Develop/client/src/utils/queries.js",[],[],{"ruleId":"76","severity":1,"message":"77","line":8,"column":8,"nodeType":"78","messageId":"79","endLine":8,"endColumn":12},{"ruleId":"76","severity":1,"message":"80","line":1,"column":17,"nodeType":"78","messageId":"79","endLine":1,"endColumn":25},{"ruleId":"76","severity":1,"message":"81","line":1,"column":27,"nodeType":"78","messageId":"79","endLine":1,"endColumn":36},{"ruleId":"76","severity":1,"message":"82","line":11,"column":21,"nodeType":"78","messageId":"79","endLine":11,"endColumn":32},{"ruleId":"76","severity":1,"message":"83","line":11,"column":34,"nodeType":"78","messageId":"79","endLine":11,"endColumn":43},{"ruleId":"84","severity":1,"message":"85","line":15,"column":14,"nodeType":"78","messageId":"86","endLine":15,"endColumn":19},{"ruleId":"84","severity":1,"message":"85","line":16,"column":19,"nodeType":"78","messageId":"86","endLine":16,"endColumn":24},{"ruleId":"76","severity":1,"message":"87","line":19,"column":22,"nodeType":"78","messageId":"79","endLine":19,"endColumn":26},{"ruleId":"76","severity":1,"message":"88","line":32,"column":22,"nodeType":"78","messageId":"79","endLine":32,"endColumn":27},{"ruleId":"76","severity":1,"message":"87","line":95,"column":15,"nodeType":"78","messageId":"79","endLine":95,"endColumn":19},{"ruleId":"76","severity":1,"message":"88","line":15,"column